    "#stack witrh limited size\n",
    "class stack:\n",
    "    def __init__(self,maxsize):\n",
    "        self.maxsize=maxsize\n",
    "        self.list=[]\n",
    "    def __str__(self):\n",
    "        values=self.list.reverse()\n",
    "        values=[str(x) for x in self.list]\n",
    "        return '\\n'.join(values)\n",
    "\n"
